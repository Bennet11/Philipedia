<%= form_for @wiki do |f| %>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title, class: 'form-control', placeholder: 'Wiki Title' %>
  </div>

  <div class="field">
    <%= f.label :body %><br />
    <%= f.text_area :body, rows: 4, class: 'form-control', placeholder: 'Wiki Body' %>
  </div><br />

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
